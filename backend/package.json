{
	"name": "playtorr-backend",
	"version": "0.0.1",
	"description": "",
	"author": "",
	"private": true,
	"license": "MIT",
	"scripts": {
		"initial-installation": "npm install --legacy-peer-deps",
		"remove-node_modules": "find . -name 'node_modules' -type d -prune -print -exec rm -rf '{}' \\;",
		"remove-package-lock": "rm package-lock.json",
		"prebuild": "rimraf dist",
		"build:api": "nest build api",
		"build:auth": "nest build auth",
		"build:user": "nest build user",
		"build:minio": "nest build minio",
		"build:telegram": "nest build telegram",
		"build": "npm run build:api && npm run build:auth && npm run build:user && npm run build:telegram && npm run build:minio",
		"format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
		"start:api": "nest start api",
		"start:auth": "nest start auth",
		"start:user": "nest start user",
		"start:minio": "nest start minio",
		"start:telegram": "nest start telegram",
		"start": "npm run start:api & npm run start:auth & npm run start:user & npm run start:telegram & npm run start:minio",
		"start-dev:api": "nest start api --watch",
		"start-dev:auth": "nest start auth --watch",
		"start-dev:user": "nest start user --watch",
		"start-dev:minio": "nest start minio --watch",
		"start-dev:telegram": "nest start telegram --watch",
		"start:dev": "npm run start-dev:api & npm run start-dev:auth & npm run start-dev:user & npm run start-dev:telegram & npm run start-dev:minio",
		"start-debug:api": "nest start api --debug --watch",
		"start-debug:auth": "nest start auth --debug --watch",
		"start-debug:user": "nest start user --debug --watch",
		"start-debug:minio": "nest start minio --debug --watch",
		"start-debug:telegram": "nest start telegram --debug --watch",
		"start:debug": "npm run start-debug:api & npm run start-debug:auth & npm run start-debug:user & npm run start-debug:telegram & npm run start-debug:minio",
		"start-prod:api": "node dist/api/main",
		"start-prod:auth": "node dist/auth/main",
		"start-prod:user": "node dist/user/main",
		"start-prod:minio": "node dist/minio/main",
		"start-prod:telegram": "node dist/telegram/main",
		"start:prod": "npm run start-prod:api & npm run start-prod:auth & npm run start-prod:user & npm run start-prod:telegram & npm run start-prod:minio",
		"lint-staged:api": "cd apps/api && lint-staged --no-stash",
		"lint-staged:auth": "cd apps/auth && lint-staged --no-stash",
		"lint-staged:user": "cd apps/user && lint-staged --no-stash",
		"lint-staged:minio": "cd apps/minio && lint-staged --no-stash",
		"lint-staged:telegram": "cd apps/telegram && lint-staged --no-stash",
		"lint-staged": "npm run lint-staged:api && npm run lint-staged:auth && npm run lint-staged:user && npm run lint-staged:telegram && npm run lint-staged:minio",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"check-types:api": "cd apps/api && tsc --noEmit",
		"check-types:auth": "cd apps/auth && tsc --noEmit",
		"check-types:user": "cd apps/user && tsc --noEmit",
		"check-types:minio": "cd apps/minio && tsc --noEmit",
		"check-types:telegram": "cd apps/telegram && tsc --noEmit",
		"check-types": "npm run check-types:api & npm run check-types:auth & npm run check-types:user & npm run check-types:telegram & npm run check-types:minio",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config apps/auth/test/jest-e2e.json"
	},
	"dependencies": {
		"@nestjs/common": "^9.1.2",
		"@nestjs/config": "^2.2.0",
		"@nestjs/core": "^9.1.2",
		"@nestjs/jwt": "^9.0.0",
		"@nestjs/mapped-types": "^1.2.0",
		"@nestjs/microservices": "^9.1.2",
		"@nestjs/mongoose": "^9.2.0",
		"@nestjs/passport": "^9.0.0",
		"@nestjs/platform-express": "^9.1.2",
		"@nestjs/swagger": "^6.1.2",
		"bcryptjs": "^2.4.3",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.13.2",
		"connect-redis": "^6.1.3",
		"date-fns": "^2.29.3",
		"express-session": "^1.17.3",
		"fs-extra": "^10.1.0",
		"ioredis": "^5.2.3",
		"minio": "^7.0.32",
		"mongoose": "^6.6.1",
		"nestjs-pino": "^3.1.1",
		"nestjs-rmq": "^2.8.0",
		"passport": "^0.6.0",
		"passport-jwt": "^4.0.0",
		"passport-local": "^1.0.0",
		"pino": "^8.6.1",
		"pino-http": "^8.2.1",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^3.0.2",
		"rxjs": "^7.5.6",
		"swagger-ui-express": "^4.5.0",
		"telegraf": "^4.9.2",
		"uuid": "^9.0.0"
	},
	"devDependencies": {
		"@nestjs/cli": "^9.1.3",
		"@nestjs/schematics": "^9.0.3",
		"@nestjs/testing": "^9.1.2",
		"@types/express": "^4.17.14",
		"@types/fs-extra": "^9.0.13",
		"@types/jest": "29.0.3",
		"@types/minio": "^7.0.14",
		"@types/multer": "^1.4.7",
		"@types/node": "^18.7.19",
		"@types/pino": "^7.0.5",
		"@types/supertest": "^2.0.12",
		"@types/uuid": "^8.3.4",
		"@typescript-eslint/eslint-plugin": "^5.38.0",
		"@typescript-eslint/parser": "^5.38.0",
		"eslint": "^8.22.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"jest": "29.0.3",
		"prettier": "^2.7.1",
		"source-map-support": "^0.5.21",
		"supertest": "^6.2.4",
		"ts-jest": "29.0.2",
		"ts-loader": "^9.4.1",
		"ts-node": "^10.9.1",
		"tsconfig-paths": "4.1.0",
		"typescript": "^4.8.3"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": ".",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "./coverage",
		"testEnvironment": "node",
		"roots": [
			"<rootDir>/apps/",
			"<rootDir>/libs/"
		],
		"moduleNameMapper": {
			"^@app/contracts(|/.*)$": "<rootDir>/libs/contracts/src/$1",
			"^@app/interfaces(|/.*)$": "<rootDir>/libs/interfaces/src/$1",
			"^@app/configs(|/.*)$": "<rootDir>/libs/configs/src/$1",
			"^@app/dto(|/.*)$": "<rootDir>/libs/dto/src/$1"
		}
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": "npm run lint",
		"**/*.ts": "tsc-files --noEmit"
	}
}
