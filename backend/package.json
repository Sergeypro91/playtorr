{
	"name": "playtorr-backend",
	"version": "0.0.1",
	"description": "",
	"author": "",
	"private": true,
	"license": "MIT",
	"scripts": {
		"initial-installation": "npm install --legacy-peer-deps",
		"remove-node_modules": "find . -name 'node_modules' -type d -prune -print -exec rm -rf '{}' \\;",
		"remove-package-lock": "rm package-lock.json",
		"prebuild": "rimraf dist",
		"build:api": "nest build api",
		"build:main-api": "nest build main-api",
		"build:telegram": "nest build telegram",
		"build:minio": "nest build minio",
		"build": "npm run build:api && npm run build:main-api && npm run build:telegram && npm run build:minio",
		"format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
		"start:api": "nest start api",
		"start:main-api": "nest start main-api",
		"start:telegram": "nest start telegram",
		"start:minio": "nest start minio",
		"start": "npm run start:api & npm run start:main-api & npm run start:telegram & npm run start:minio",
		"start-dev:api": "nest start api --watch",
		"start-dev:main-api": "nest start main-api --watch",
		"start-dev:telegram": "nest start telegram --watch",
		"start-dev:minio": "nest start minio --watch",
		"start:dev": "npm run start-dev:api & npm run start-dev:main-api & npm run start-dev:telegram & npm run start-dev:minio",
		"start-debug:api": "nest start api --debug --watch",
		"start-debug:main-api": "nest start main-api --debug --watch",
		"start-debug:telegram": "nest start telegram --debug --watch",
		"start-debug:minio": "nest start minio --debug --watch",
		"start:debug": "npm run start-debug:api & npm run start-debug:main-api & npm run start-debug:telegram & npm run start-debug:minio",
		"start-prod:api": "node dist/api/main",
		"start-prod:main-api": "node dist/main-api/main",
		"start-prod:telegram": "node dist/telegram/main",
		"start-prod:minio": "node dist/minio/main",
		"start:prod": "npm run start-prod:api & npm run start-prod:main-api & npm run start-prod:telegram & npm run start-prod:minio",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"check-types:api": "cd apps/api && tsc --noEmit",
		"check-types:main-api": "cd apps/main-api && tsc --noEmit",
		"check-types:telegram": "cd apps/telegram && tsc --noEmit",
		"check-types:minio": "cd apps/minio && tsc --noEmit",
		"check-types": "npm run check-types:api & npm run check-types:main-api & npm run check-types:telegram & npm run check-types:minio",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./apps/main-api/test/jest-e2e.json"
	},
	"dependencies": {
		"@nestjs/common": "^9.1.2",
		"@nestjs/config": "^2.2.0",
		"@nestjs/core": "^9.1.2",
		"@nestjs/jwt": "^9.0.0",
		"@nestjs/microservices": "^9.1.2",
		"@nestjs/mongoose": "^9.2.0",
		"@nestjs/passport": "^9.0.0",
		"@nestjs/platform-express": "^9.1.2",
		"bcryptjs": "^2.4.3",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.13.2",
		"connect-redis": "^6.1.3",
		"date-fns": "^2.29.3",
		"express-session": "^1.17.3",
		"ioredis": "^5.2.3",
		"minio": "^7.0.32",
		"mongoose": "^6.6.1",
		"nestjs-rmq": "^2.8.0",
		"passport": "^0.6.0",
		"passport-jwt": "^4.0.0",
		"passport-local": "^1.0.0",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^3.0.2",
		"rxjs": "^7.5.6",
		"telegraf": "^4.9.2"
	},
	"devDependencies": {
		"@nestjs/cli": "^9.1.3",
		"@nestjs/schematics": "^9.0.3",
		"@nestjs/testing": "^9.1.2",
		"@types/express": "^4.17.14",
		"@types/jest": "29.0.3",
		"@types/multer": "^1.4.7",
		"@types/node": "^18.7.19",
		"@types/supertest": "^2.0.12",
		"@typescript-eslint/eslint-plugin": "^5.38.0",
		"@typescript-eslint/parser": "^5.38.0",
		"eslint": "^8.22.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"jest": "29.0.3",
		"prettier": "^2.7.1",
		"source-map-support": "^0.5.21",
		"supertest": "^6.2.4",
		"ts-jest": "29.0.2",
		"ts-loader": "^9.4.1",
		"ts-node": "^10.9.1",
		"tsconfig-paths": "4.1.0",
		"typescript": "^4.8.3"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": ".",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "./coverage",
		"testEnvironment": "node",
		"roots": [
			"<rootDir>/apps/",
			"<rootDir>/libs/"
		],
		"moduleNameMapper": {
			"^@app/contracts(|/.*)$": "<rootDir>/libs/contracts/src/$1",
			"^@app/interfaces(|/.*)$": "<rootDir>/libs/interfaces/src/$1",
			"^@app/configs(|/.*)$": "<rootDir>/libs/configs/src/$1",
			"^@app/dto(|/.*)$": "<rootDir>/libs/dto/src/$1"
		}
	}
}
