{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/main-api/src",
  "monorepo": true,
  "root": "apps/main-api",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/main-api/tsconfig.app.json"
  },
  "projects": {
    "main-api": {
      "type": "application",
      "root": "apps/main-api",
      "entryFile": "main",
      "sourceRoot": "apps/main-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/main-api/tsconfig.app.json"
      }
    },
    "contracts": {
      "type": "library",
      "root": "libs/contracts",
      "entryFile": "index",
      "sourceRoot": "libs/contracts/src",
      "compilerOptions": {
        "tsConfigPath": "libs/contracts/tsconfig.lib.json"
      }
    },
    "interfaces": {
      "type": "library",
      "root": "libs/interfaces",
      "entryFile": "index",
      "sourceRoot": "libs/interfaces/src",
      "compilerOptions": {
        "tsConfigPath": "libs/interfaces/tsconfig.lib.json"
      }
    },
    "configs": {
      "type": "library",
      "root": "libs/configs",
      "entryFile": "index",
      "sourceRoot": "libs/configs/src",
      "compilerOptions": {
        "tsConfigPath": "libs/configs/tsconfig.lib.json"
      }
    },
    "api": {
      "type": "application",
      "root": "apps/api",
      "entryFile": "main",
      "sourceRoot": "apps/api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api/tsconfig.app.json"
      }
    },
    "telegram": {
      "type": "application",
      "root": "apps/telegram",
      "entryFile": "main",
      "sourceRoot": "apps/telegram/src",
      "compilerOptions": {
        "tsConfigPath": "apps/telegram/tsconfig.app.json"
      }
    },
    "minio": {
      "type": "application",
      "root": "apps/minio",
      "entryFile": "main",
      "sourceRoot": "apps/minio/src",
      "compilerOptions": {
        "tsConfigPath": "apps/minio/tsconfig.app.json"
      }
    }
  }
}