{
	"$schema": "https://json.schemastore.org/nest-cli",
	"collection": "@nestjs/schematics",
	"sourceRoot": "apps/api/src",
	"monorepo": true,
	"root": "apps/api",
	"compilerOptions": {
		"builder": "swc",
		"typeCheck": true,
		"webpack": true,
		"tsConfigPath": "apps/auth/tsconfig.app.json",
		"deleteOutDir": true,
		"plugins": [
			"@nestjs/swagger/plugin"
		]
	},
	"projects": {
		"api": {
			"type": "application",
			"root": "apps/api",
			"entryFile": "main",
			"sourceRoot": "apps/api/src",
			"compilerOptions": {
				"tsConfigPath": "apps/api/tsconfig.app.json"
			}
		},
		"auth": {
			"type": "application",
			"root": "apps/auth",
			"entryFile": "main",
			"sourceRoot": "apps/auth/src",
			"compilerOptions": {
				"tsConfigPath": "apps/auth/tsconfig.app.json"
			}
		},
		"mailer": {
			"type": "application",
			"root": "apps/mailer",
			"entryFile": "main",
			"sourceRoot": "apps/mailer/src",
			"compilerOptions": {
				"tsConfigPath": "apps/mailer/tsconfig.app.json"
			}
		},
		"minio": {
			"type": "application",
			"root": "apps/minio",
			"entryFile": "main",
			"sourceRoot": "apps/minio/src",
			"compilerOptions": {
				"tsConfigPath": "apps/minio/tsconfig.app.json"
			}
		},
		"parser": {
			"type": "application",
			"root": "apps/parser",
			"entryFile": "main",
			"sourceRoot": "apps/parser/src",
			"compilerOptions": {
				"tsConfigPath": "apps/parser/tsconfig.app.json"
			}
		},
		"person": {
			"type": "application",
			"root": "apps/person",
			"entryFile": "main",
			"sourceRoot": "apps/person/src",
			"compilerOptions": {
				"tsConfigPath": "apps/person/tsconfig.app.json"
			}
		},
		"picture": {
			"type": "application",
			"root": "apps/picture",
			"entryFile": "main",
			"sourceRoot": "apps/picture/src",
			"compilerOptions": {
				"tsConfigPath": "apps/picture/tsconfig.app.json"
			}
		},
		"publicPreview": {
			"type": "application",
			"root": "apps/publicPreview",
			"entryFile": "main",
			"sourceRoot": "apps/publicPreview/src",
			"compilerOptions": {
				"tsConfigPath": "apps/publicPreview/tsconfig.app.json"
			}
		},
		"tmdb": {
			"type": "application",
			"root": "apps/tmdb",
			"entryFile": "main",
			"sourceRoot": "apps/tmdb/src",
			"compilerOptions": {
				"tsConfigPath": "apps/tmdb/tsconfig.app.json"
			}
		},
		"users": {
			"type": "application",
			"root": "apps/users",
			"entryFile": "main",
			"sourceRoot": "apps/users/src",
			"compilerOptions": {
				"tsConfigPath": "apps/users/tsconfig.app.json"
			}
		},
		"webtorrent": {
			"type": "application",
			"root": "apps/webtorrent",
			"entryFile": "main",
			"sourceRoot": "apps/webtorrent/src",
			"compilerOptions": {
				"tsConfigPath": "apps/webtorrent/tsconfig.app.json"
			}
		},
		"common": {
			"type": "library",
			"root": "libs/common",
			"entryFile": "index",
			"sourceRoot": "libs/common/src",
			"compilerOptions": {
				"tsConfigPath": "libs/common/tsconfig.lib.json"
			}
		}
	}
}
