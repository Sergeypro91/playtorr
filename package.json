{
  "name": "playtor",
  "version": "0.0.1",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "backend/**/*",
    "frontend/*",
    "tooling/*"
  ],
  "scripts": {
    "install": "npm install --legacy-peer-deps",
    "clear-node_modules": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +",
    "start-docker-services": "cp backend/.env docker && cd docker && docker-compose up",
    "start-communication-service": "cd backend/api-communication && npm start",
    "start-main-api-service": "cd backend/main-api-service && npm start",
    "start-movie-processioning-service": "cd backend/movie-processioning-service && npm start",
    "start": "npm run start-communication-service & npm run start-main-api-service & npm run start-movie-processioning-service",
    "start:dev-communication-service": "cd backend/api-communication && npm start:dev",
    "start:dev-main-api-service": "cd backend/main-api-service && npm start:dev",
    "start:dev-movie-processioning-service": "cd backend/movie-processioning-service && npm start:dev",
    "start:dev": "npm run start:dev-communication-service & npm run start:dev-main-api-service & npm run start:dev-movie-processioning-service",
    "start:prod-communication-service": "cd backend/api-communication && npm start:prod",
    "start:prod-main-api-service": "cd backend/main-api-service && npm start:prod",
    "start:prod-movie-processioning-service": "cd backend/movie-processioning-service && npm start:prod",
    "start:prod": "npm run start:prod-communication-service & npm run start:prod-main-api-service & npm run start:prod-movie-processioning-service",
    "lint-communication-service": "cd backend/api-communication && npm run lint",
    "lint-main-api-service": "cd backend/main-api-service && npm run lint",
    "lint-movie-processioning-service": "cd backend/movie-processioning-service && npm run lint",
    "lint": "npm run lint-communication-service && npm run lint-main-api-service && npm run lint-movie-processioning-service"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/connect-redis": "^0.0.18",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/jest": "27.5.0",
    "@types/minio": "^7.0.13",
    "@types/mongoose": "^5.11.97",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  }
}
